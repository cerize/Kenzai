<div class="project-details">
  <h1><%= @project.title %></h1>
  <p><%= @project.description %></p>
  <p>Creator: <%= @project.creator.full_name%></p>
  <%= link_to "Edit", edit_project_path(@project), remote: true %>
  <p>
    Starting date: <%= @project.start_date %>
    Ending date: <%= @project.end_date %>
  </p>
  <%= link_to "Delete",
  project_path(@project),
  method: :delete,
  data: {confirm: "Are you sure you want to delete this project? This action cannot be undone"} %>
</div>
